{
  "hosting": [
    {
      "target":  "main",
      "public": "build",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    },
    {
      "target": "testing-int",
      "public": "build"
    }
  ]
}
